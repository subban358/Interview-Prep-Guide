for _ in range(int(input())):
    n = int(input())
    arr = [int(x) for x in input().split()]
    c0, c1, c2 = 0, 0, 0
    for i in arr:
        if i == 0:
            c0+=1
        elif i==1:
            c1+=1
        else:
            c2+=1
    i = 0
    while c0:
        arr[i] = 0
        c0 -= 1
        i+=1
    while c1:
        arr[i] = 1
        c1-=1
        i+=1
    while c2:
        arr[i] = 2
        c2-=1
        i+=1
    print(*arr)    
        
